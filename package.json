    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "electron": "^6.1.9",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.6",
